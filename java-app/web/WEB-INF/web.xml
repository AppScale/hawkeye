<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>ProjectHandlerServlet</servlet-name>
        <servlet-class>com.appscale.hawkeye.datastore.ProjectHandlerServlet
        </servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModuleHandlerServlet</servlet-name>
        <servlet-class>com.appscale.hawkeye.datastore.ModuleHandlerServlet
        </servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectModuleHandlerServlet</servlet-name>
        <servlet-class>com.appscale.hawkeye.datastore.ProjectModuleHandlerServlet
        </servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProjectKeyHandlerServlet</servlet-name>
        <servlet-class>com.appscale.hawkeye.datastore.ProjectKeyHandlerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EntityNameHandlerServlet</servlet-name>
        <servlet-class>com.appscale.hawkeye.datastore.EntityNameHandlerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProjectHandlerServlet</servlet-name>
        <url-pattern>/java/datastore/project</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModuleHandlerServlet</servlet-name>
        <url-pattern>/java/datastore/module</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectModuleHandlerServlet</servlet-name>
        <url-pattern>/java/datastore/project_modules</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProjectKeyHandlerServlet</servlet-name>
        <url-pattern>/java/datastore/project_keys</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EntityNameHandlerServlet</servlet-name>
        <url-pattern>/java/datastore/entity_names</url-pattern>
    </servlet-mapping>
</web-app>
